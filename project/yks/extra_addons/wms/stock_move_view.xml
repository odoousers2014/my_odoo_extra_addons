<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data>


        <!-- stock.move tree for stock.picking.out.form -->
        <record model="ir.ui.view" id="inherit_view_move_picking_tree">
            <field name="name">wms.inherit_view_move_tree</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_picking_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//tree" position="inside">
                	<button name='get_stock_available' string='Stock Available for multi Location' type='object' icon='terp-stock_zoom'/>
                </xpath>
            </field>
        </record>


    
</data>
</openerp>